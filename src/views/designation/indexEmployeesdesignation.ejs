<%-include('../admin/partials/header.ejs')%>

      <div class="flex h-full bg-gray-50 dark:bg-gray-900" :class="{ 'overflow-hidden': isSideMenuOpen }">
        <%-include('../admin/partials/navegation.ejs')%>
        <%-include('../admin/partials/search.ejs')%>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/iconoir-icons/iconoir@main/css/iconoir.css">
<%if(typeof data === 'string') {%>
    <h1 class="text-center font-bold p-3 mt-5 bg-blue-600 text-white rounded-full"><%=data%></h1>
<%}else if(typeof data != 'undefined') {%>
    <section class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-4 py-12">
        <div class="text-center pb-12">
            <h2 class="text-base font-bold text-indigo-600">
                
            </h2>
            <%if(data.length > 0) {%>
                <h1 id="buttomH1" class="bg-blue-600 p-3 mb-10  rounded-lg flex justify-center items-center text-white">Tus evaluaciones de circuitos.<svg class="w-30 h-9 ml-5" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns="http://www.w3.org/2000/svg" height="24" width="24" version="1.1" xmlns:cc="http://creativecommons.org/ns#" xmlns:dc="http://purl.org/dc/elements/1.1/">
                    <g transform="translate(0 -1028.4)">
                     <path d="m22 12c0 5.523-4.477 10-10 10-5.5228 0-10-4.477-10-10 0-5.5228 4.4772-10 10-10 5.523 0 10 4.4772 10 10z" transform="translate(0 1029.4)" fill="#27ae60"/>
                     <path d="m22 12c0 5.523-4.477 10-10 10-5.5228 0-10-4.477-10-10 0-5.5228 4.4772-10 10-10 5.523 0 10 4.4772 10 10z" transform="translate(0 1028.4)" fill="#2ecc71"/>
                     <path d="m16 1037.4-6 6-2.5-2.5-2.125 2.1 2.5 2.5 2 2 0.125 0.1 8.125-8.1-2.125-2.1z" fill="#27ae60"/>
                     <path d="m16 1036.4-6 6-2.5-2.5-2.125 2.1 2.5 2.5 2 2 0.125 0.1 8.125-8.1-2.125-2.1z" fill="#ecf0f1"/>
                    </g>
                   </svg></h1> 
                
        </div>
        <div class="grid grid-cols-1 sm:grid-cols-2 md:grid-cols-2 lg:grid-cols-3 gap-6">
            <%data.forEach((Elemento=> {%>
                <div class="w-full bg-white rounded-lg p-12 flex flex-col justify-center items-center">
                    <div class="mb-8">
                        <img class="object-center object-cover rounded-full h-36 w-36" src="/images/task.png" alt="photo">
                    </div>
                    <div class="text-left">
                        <p class="text- text-black-700">Identificador: <strong><%=Elemento.id%></strong></p>
                        <p class="text-base text-black-400 font-normal">Circuito: <strong><%=Elemento.circuit%></strong></p>
                        <p class="text-base text-black-400 font-normal border-b">Estado: <strong><%=Elemento.estate%></strong></p>
                        <p class="text-black-400 font-normal text-center pt-2">Descripcion: <%=Elemento.description%></p> 
                        <p class="pt-2 text-center"><a href="/designation/employees/update/<%=Elemento.id%>" class="btn bg-blue-600 flex justify-center items-center text-white hover:bg-blue-800"><i class="pr-1 fi-sr-replace"></i></a><a href="/designation/employees/delete/<%=Elemento.id%>" class="bg-red-600 btn flex items-center justify-center text-white hover:bg-red-800 mt-2"><i class="pr-1 fi-sr-circle-trash"></i></a></p>   
                    </div>
                </div>
            <%}))%>
        </div>
    </section>
    </div>
    <%}%>
<%}else{%>
    <h1 class="bg-red-600 p-3  rounded-lg flex justify-center text-white">No has creado una evalucacion del circuito.<svg class="w-30 ml-3 h-5" id="Layer_1" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 48 48"><defs><style>.cls-1{fill:#fac100;}.cls-2{fill:#ff9500;}.cls-3{fill:#2d4a60;}.cls-4{fill:#263f52;}</style></defs><title>Crisis</title><path class="cls-1" d="M44.5,34.5,28.33,6.5a5,5,0,0,0-8.66,0L3.5,34.5A5,5,0,0,0,7.834,42H40.166a5,5,0,0,0,4.33-7.5Z"/><path class="cls-2" d="M34,37A24.006,24.006,0,0,1,11.344,20.921L3.5,34.5A5,5,0,0,0,7.834,42H40.166a5,5,0,0,0,4.364-7.431A23.9,23.9,0,0,1,34,37Z"/><path class="cls-3" d="M24,30A2.1,2.1,0,0,0,26.1,28.1l1.18-11.8A3,3,0,0,0,24.29,13h-.58a3,3,0,0,0-2.985,3.3L21.905,28.1A2.1,2.1,0,0,0,24,30Z"/><circle class="cls-3" cx="24" cy="34.5" r="3.5"/><path class="cls-4" d="M24,38a3.5,3.5,0,0,0,3.154-2.009,23.838,23.838,0,0,1-6.326-2.949A3.46,3.46,0,0,0,20.5,34.5,3.5,3.5,0,0,0,24,38Z"/></svg></h1>

<%}%>    



<script>
    let btn = document.getElementById('buttomH1');
    

    btn.addEventListener('click',(e)=>{
        btn.textContent = "ok entendido!";
        setTimeout(() => {
            btn.classList.toggle('hidden');
        },0,20);
    });


</script>

<%if(typeof alert != 'undefined') {%>
    <script>
        let timerInterval
        Swal.fire({
            title: "<%=alertTitle%>",
            html: "<%=alertHtml%> <b></b> millisegundos.",
            timer: 2500,
            timerProgressBar: true,
            didOpen: () => {
                Swal.showLoading()
                const b = Swal.getHtmlContainer().querySelector('b')
                timerInterval = setInterval(() => {
                    b.textContent = Swal.getTimerLeft()
                }, 100)
            },
            willClose: () => {
                clearInterval(timerInterval)
            }
            }).then((result) => {
            /* Read more about handling dismissals below */
            if (result.dismiss === Swal.DismissReason.timer) {
                window.location = "<%=ruta%>"
            }
        })
    </script>
<%}%>   

<style>
    #buttomH1:hover{
        cursor: pointer;
    }
</style>